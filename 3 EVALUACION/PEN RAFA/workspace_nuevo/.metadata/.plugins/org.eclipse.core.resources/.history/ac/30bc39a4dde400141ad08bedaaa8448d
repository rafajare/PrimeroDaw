
public class GuitarraElectrica extends InstrumentoDeCuerda{

	public GuitarraElectrica(String n_,int numCuerdas_){
		super( n_,numCuerdas_);
	
	}
	
	public GuitarraElectrica(){
		this("zzz",0);
	}
	
	public void getTocar(){
		
		System.out.println("Esta" +getNombre()+ "tiene "+getNumCuerdas()+" número de cuerdas");
	}
	

	

}
